<form action="/api/users" method="post">
  <%= auth_token %>

  <label>Email
    <input type="text" name="user[email]" />
  </label>
  <br>

  <label>Name
    <input type="text" name="user[Name]" />
  </label>
  <br>
  <label>Password
    <input type="text" name="user[password]" />
  </label>
  <br>

  <button>Register</button>

</form>
